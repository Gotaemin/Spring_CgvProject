<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.tm.cgv.member.MemberDAO">

	<insert id="infoJoin" parameterType="com.tm.cgv.member.MemberDTO">
		insert into member values(#{id},#{pwd},#{name},#{birth},#{phone},#{email})
	</insert>
	
	<select id="infoLogin" parameterType="com.tm.cgv.member.MemberDTO" resultType="com.tm.cgv.member.MemberDTO">
		select * from member where id=#{id} and pwd=#{pwd}
	</select>

</mapper>